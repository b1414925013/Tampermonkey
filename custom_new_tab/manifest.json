{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "一个自定义的新标签页插件",
  "icons": {
    "128": "icons/icon.png"
  },
  
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "bookmarks"],
  "action": {
    "default_icon": "icons/icon.png"
  }
}